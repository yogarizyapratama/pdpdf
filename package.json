    "deploy:vercel": "./deploy-vercel.sh",
    "deploy:check": "npm run build && npm run lint",
    "vercel:build": "NEXT_PUBLIC_APP_URL=https://pdpdf.vercel.app npm run build",
    "postbuild": "echo 'âœ… Build completed for PDPDF - Ready for Vercel deployment!'"
